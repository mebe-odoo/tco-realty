<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="terminate_tenancy_cron" model="ir.cron">
            <field name="name">Tenancies: Terminate expired tenancies</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now().replace(hour=2, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="doall" eval="False" />
            <field name="model_id" ref="model_realty_tenancy" />
            <field name="code">model._cron_check_termination()</field>
            <field name="state">code</field>
        </record>

    </data>


</odoo>