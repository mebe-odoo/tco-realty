<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id='tenancy_termination_server_action' model='ir.actions.server'>
            <field name="name">Terminate Tenancies</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="model_realty_tenancy"/>
            <field name="binding_model_id" ref="model_realty_tenancy"/>
            <field name='state'>code</field>
            <field name='code'>
                for rec in records:
                    records.action_terminate()
            </field>
        </record>

        <record id="tenancy_portal_url" model="ir.actions.act_url">
            <field name="name">Tenancy Portal URL</field>
            <field name="type">ir.actions.act_url</field>
            <field name="url">https://portal.fake-agency.com/tenancies</field>
        </record>

    </data>
</odoo>